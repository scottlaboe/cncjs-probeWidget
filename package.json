{
  "name": "cncjs-widget-boilerplate",
  "version": "1.0.0",
  "description": "Creating custom widgets for CNCjs.",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "npm run eslint && npm test && npm run clean && npm run build",
    "build": "cross-env OUTPUT_PATH=dist/v1 webpack --config webpack.config.js",
    "clean": "del-cli dist",
    "deploy": "./scripts/deploy.sh",
    "demo": "http-server -p 8080 dist",
    "eslint": "eslint ./src",
    "test": "tap test/*.js --node-arg=--require --node-arg=babel-register --node-arg=--require --node-arg=babel-polyfill",
    "coveralls": "tap test/*.js --coverage --coverage-report=text-lcov --nyc-arg=--require --nyc-arg=babel-register --nyc-arg=--require --nyc-arg=babel-polyfill | coveralls",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --hot --inline --host 0.0.0.0 --port 5000 --content-base dist",
    "pack": ""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cncjs/cncjs-widget-boilerplate.git"
  },
  "author": "nobody <nobody@example.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cncjs/cncjs-widget-boilerplate/issues"
  },
  "homepage": "https://github.com/cncjs/cncjs-widget-boilerplate",
  "keywords": [
    "cncjs",
    "widget",
    "boilerplate",
    "react"
  ],
  "peerDependencies": {},
  "dependencies": {
    "classnames": "~2.2.5",
    "cncjs-controller": "~1.1.0",
    "detect-browser": "~1.10.0",
    "font-awesome": "~4.7.0",
    "lodash.get": "~4.4.2",
    "lodash.mapvalues": "~4.6.0",
    "node-emoji": "~1.8.1",
    "normalize.css": "~7.0.0",
    "prop-types": ">=15.0.0",
    "qs": "~6.5.1",
    "react": "^0.14.0 || <17.0.0-0",
    "react-dom": "^0.14.0 || <17.0.0-0",
    "semver": "~5.4.1",
    "socket.io-client": "~1.7.4",
    "styled-components": "~2.1.2",
    "universal-logger": "~1.0.1",
    "universal-logger-browser": "~1.0.2"
  },
  "devDependencies": {
    "babel-cli": "~6.26.0",
    "babel-core": "~6.26.0",
    "babel-eslint": "~8.0.0",
    "babel-loader": "~7.1.2",
    "babel-plugin-transform-decorators-legacy": "~1.3.4",
    "babel-preset-es2015": "~6.24.1",
    "babel-preset-react": "~6.24.1",
    "babel-preset-stage-0": "~6.24.1",
    "coveralls": "~2.13.1",
    "cross-env": "~5.1.1",
    "css-loader": "~0.28.7",
    "del-cli": "~1.1.0",
    "eslint": "~4.7.1",
    "eslint-config-trendmicro": "~1.0.0",
    "eslint-loader": "~1.9.0",
    "eslint-plugin-import": "~2.7.0",
    "eslint-plugin-jsx-a11y": "~5.1.1",
    "eslint-plugin-react": "~7.3.0",
    "file-loader": "~0.11.2",
    "find-imports": "~0.5.2",
    "gh-pages": "~1.0.0",
    "html-webpack-plugin": "~2.30.1",
    "http-server": "~0.10.0",
    "nib": "~1.1.2",
    "style-loader": "~0.18.2",
    "stylint": "~1.5.9",
    "stylint-loader": "~1.0.0",
    "stylus-loader": "~3.0.1",
    "tap": "~10.7.2",
    "url-loader": "~0.5.9",
    "webpack": "~3.6.0",
    "webpack-dev-server": "~2.8.2",
    "write-file-webpack-plugin": "~4.1.0"
  }
}
